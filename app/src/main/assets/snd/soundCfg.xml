	<!-- 高顺 -->
	<Sound type="2" ID="403" branch="1" text="攻无不克战无不胜。"/>
	<Sound type="2" ID="403" branch="2" text="破阵斩将，易如反掌。"/>
	<Sound type="2" ID="404" branch="1" text="避嫌远疑，所以无误"/>
