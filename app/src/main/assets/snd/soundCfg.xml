	<!-- 曹植 -->
	<Sound type="2" ID="401" branch="1" text="这些都是我的。"/>
	<Sound type="2" ID="401" branch="2" text="别着急扔，给我就好。"/>
	<Sound type="2" ID="402" branch="1" text="置酒高殿上，亲友从我游。"/>
